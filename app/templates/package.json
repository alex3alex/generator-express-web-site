{
    "name": "<%= _.slugify(appname) %>",
    "version": "<%= version %>",
    "private": true,
    "main": "app/index.js",
    "scripts": {
        "status": "node_modules/.bin/forever list",
        "stop": "node_modules/.bin/forever stop ./app/index.js",
        "update": "npm install && bower install --allow-root && grunt build",
        "start": "npm stop && grunt build && node_modules/.bin/forever start ./app/index.js --port 3334"
    },
    "dependencies": {
        "async": "~0.9.0",
        "body-parser": "~1.10.0",
