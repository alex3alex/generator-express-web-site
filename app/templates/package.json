        "glob": "~4.3.1",
        "jade": "~1.8.1",
        "lodash": "~2.4.1",
        "method-override": "~2.3.0",
        "moment": "~2.9.0",
        "morgan": "~1.5.0",
        "multer": "~0.1.6",
        "optimist": "~0.6.1",
        "constantin": "~1.0.2",
        "serve-favicon": "~2.1.7",
		"forever": "~0.13.0"
    },
    "devDependencies": {
        "grunt": "^0.4.5",
        "grunt-autoprefixer": "^2.2.0",
        "grunt-bower-task": "^0.4.0",
        "grunt-combine-media-queries": "^1.0.19",
        "grunt-contrib-clean": "^0.6.0",
